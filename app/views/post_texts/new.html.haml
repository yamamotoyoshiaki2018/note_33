    .post__text__contents__center
      .post__text__contents__center__img
        = fa_icon ("image 5x")
      .post__text__contents__center__title
        %input.post__text__contents__center__title__text{:placeholder => "タイトルを入力", :size => "50", :type => "text"}/
      .post__text__contents__center__textarea
        %input.post__text__contents__center__textarea__input{:placeholder => "ノート本文", :cols => "50", :rows => "10", :type => "textarea"}/

